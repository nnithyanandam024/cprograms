#include<stdio.h>
#include<string.h>
int main()
{
    char str[200];
    int i=0,j=0;
    scanf("%[^\n]",str);
    int len=strlen(str);
    for(int i=0;str[i]!='\0';i++)
        {
            if(str[i]!='a' &&str[i]!='i' &&str[i]!='e' &&str[i]!='o' &&str[i]!='u' &&str[i]!='A' &&str[i]!='E' &&str[i]!='I' &&str[i]!='O' &&str[i]!='U')
            {
                str[j++]=str[i];
            }
                }
    str[j]='\0';
    printf("%s",str);
}
