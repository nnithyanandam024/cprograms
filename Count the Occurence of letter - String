#include<stdio.h>
#include<string.h>
int main()
{
    char str[200];
    char ch;
    int count=0;
    scanf("%[^\n]",str);
    getchar();
    scanf("%c",&ch);
    int len=strlen(str);
    for(int i=0;i<len;i++)
        {
            if(str[i]==ch)
            {
                count++;
            }
        }
    printf("%d",count);
}
